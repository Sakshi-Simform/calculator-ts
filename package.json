{
  "name": "scientificcalc-ts",
  "version": "1.0.0",
  "description": "## This  project shows the scientic calc focus area to create this ui design is:-",
  "type": "module",
  "scripts": {
    "dev": "parcel serve ./src/index.html --port 5500",
    "type-check": "tsc -b --noEmit",
    "lint": "eslint src --ext .ts --cache --fix",
    "build": "npm run type-check && npm run lint && parcel build ./src/index.html --no-autoinstall --no-optimize",
    "format": "prettier src --write",
    "clean": "rm -rf dist .parcel-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sakshi-Simform/calculator-ts.git"
  },
  "keywords": [
    "calculator",
    "typeScript",
    "parcel"
  ],
  "author": "sakshi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sakshi-Simform/calculator-ts/issues"
  },
  "homepage": "https://github.com/Sakshi-Simform/calculator-ts#readme",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "parcel": "^2.14.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
     "prettier": "^3.5.3"
  }
}
